<h1>Edit Module</h1>


<form action="/module/edit/:mid" method = "post">
    <% modules.forEach((module)=> { %>
    <label>Module ID:</label><br>
    <input type="text" name="mid" placeholder= "<%= module.mid %>"" readonly><br>
    <label>Module Name:</label>
    <input type="text" name="name" placeholder="<%= module.name %>"><br>
    <label>Credits:</label>
    <input type="text" name="credits" placeholder="<%= module.credits %>"><br>

    <input type="submit" value="Submit">

    <% }) %>
</form>